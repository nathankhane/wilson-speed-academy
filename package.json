        "@radix-ui/react-toast": "^1.1.5",
        "class-variance-authority": "^0.7.0",
        "clsx": "^2.1.1",
        "framer-motion": "^11.3.8",
        "gray-matter": "^4.0.3",
        "keen-slider": "^6.8.6",
        "lucide-react": "^0.408.0",
        "next": "14.2.5",
        "next-themes": "^0.3.0",
        "react": "^18",
        "react-dom": "^18",
        "react-hook-form": "^7.52.1",
